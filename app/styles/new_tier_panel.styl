.new_tier_panel
	
	> .overlay
		position fixed
		height 4000px
		width 100%
		left 0px
		top 0px
		background-color black
		opacity 0.7
		

	.main_cont
		position:fixed
		top 40%
		left 0
		width 100%
		& > div
				margin 0 auto
				text-align center
				width 100%
				> div
					padding:25px 50px
					border 3px solid black
					background-color white
					display inline-block
	label > strong
		display block
		padding-bottom 15px
		font-size 1.2em